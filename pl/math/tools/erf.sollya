display = hexadecimal;
prec=128;

// Tables
print("{ i, r, erf(r), 2/sqrt(pi) * exp(-r^2)}");
for i from 0 to 768 do {
  r = i / 128;
  t0 = double(erf(r));
  t1 = double(2/sqrt(pi) * exp(-r * r));
  print("{ " @ i @ ",\t" @ r @ ",\t" @ t0 @ ",\t" @ t1 @ " },");
};

// Constants
prec=52;
1/3;
1/10;
2/15;
2/9;
2/45;
2/sqrt(pi);

